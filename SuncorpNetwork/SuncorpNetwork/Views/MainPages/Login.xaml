<?xml version="1.0" encoding="UTF-8"?>
<BaseView 	xmlns="http://xamarin.com/schemas/2014/forms" 
				xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
				x:Class="SuncorpNetwork.Login"
				BackgroundColor="#0DA195"
				Padding = "20,20,20,10">
	<ContentPage.Content>
		<StackLayout>
			<Button BackgroundColor="White"
					Clicked = "loginBtnClicked"
					WidthRequest = "50"
					HeightRequest = "25"
			/>
			<Button BackgroundColor="White"
					Clicked = "registerBtnClicked"
					WidthRequest = "50"
					HeightRequest = "25"
			/>
		</StackLayout>

	</ContentPage.Content>
</BaseView>
